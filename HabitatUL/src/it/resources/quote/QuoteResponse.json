{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "quote response",
  "required": [
    "quoteId",
    "premium",
    "expirationDate"
  ],
  "properties": {
    "quoteId": {
      "description": "quote id",
      "type": "string",
      "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
    },
    "premium": {
      "description": "quote premium",
      "type": "number"
    },
    "expirationDate": {
      "description": "quote expiration date - unix epoch timestamp",
      "type": "number",
      "pattern": "^\\d{10}$"
    }
  }
}
