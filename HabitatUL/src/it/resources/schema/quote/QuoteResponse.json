{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "quote response",
  "required": [
    "quoteId",
    "premium",
    "effectivePeriod",
    "expirationDate",
    "coverageOverview"
  ],
  "properties": {
    "quoteId": {
      "description": "quote id",
      "type": "string",
      "$ref": "../Shared.json#/definitions/uuid"
    },
    "premium": {
      "description": "quote premium",
      "$ref": "../Shared.json#/definitions/money"
    },
    "effectivePeriod": {
      "description": "quote effective period",
      "type": "object",
      "properties": {
        "startDate": {
          "$ref": "../Shared.json#/definitions/date"
        },
        "endDate": {
          "$ref": "../Shared.json#/definitions/date"
        }
      }
    },
    "expirationDate": {
      "description": "quote expiration",
      "$ref": "../Shared.json#/definitions/dateTime"
    },
    "coverageOverview": {
      "description": "quote coverage overview",
      "properties": {
        "personalProperty": {
          "$ref": "../Shared.json#/definitions/amount"
        },
        "civilLiability": {
          "$ref": "../Shared.json#/definitions/amount"
        }
      }
    }
  }
}
