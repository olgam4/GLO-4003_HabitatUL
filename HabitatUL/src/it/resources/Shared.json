{
  "definitions": {
    "date": {
      "type": "string",
      "pattern": "^(\\d{4}-(0?[1-9]|[1][012])-([12][0-9]|3[01]|0?[1-9]))$"
    },
    "dateTime": {
      "type": "string",
      "pattern": "^((19|20)[0-9][0-9])[-](0[1-9]|1[012])[-](0[1-9]|[12][0-9]|3[01])[T]([01][0-9]|[2][0-3])[:]([0-5][0-9])[:]([0-5][0-9]).([0-9]{3})([+|-]([01][0-9]|[2][0-3])[:]([0-5][0-9]))?Z?(\\[[a-zA-Z\\/]+\\])?$"
    },
    "epochTime": {
      "type": "number",
      "pattern": "^\\d{10}$"
    },
    "jwt": {
      "type": "string",
      "pattern": "^[A-Za-z0-9-_=]+.[A-Za-z0-9-_=]+.?[A-Za-z0-9-_.+/=]*$"
    },
    "money": {
      "type": "number",
      "pattern": "^\\d*\\.\\d{2}$"
    },
    "uuid": {
      "type": "string",
      "pattern": "[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}"
    }
  }
}
